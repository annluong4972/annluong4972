<apex:page title="Create Contact" standardController="Contact" >
    <apex:form >
        <apex:pageBlock title="Contact Data Entry">
        	<apex:pageBlockSection title="Contact Information">
                <apex:inputField label="Contact First Name" value="{! Contact.FirstName}" /><br/>
                <apex:inputField label="Contact Last Name" value="{! Contact.LastName}"/> <br/>
                <apex:inputField label="Contact Email" value="{!Contact.Email}" />    
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
            	<apex:commandButton action="{! save}" value="Save" />
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
</apex:page>